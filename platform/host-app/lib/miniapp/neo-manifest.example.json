{
  "$schema": "./neo-manifest.schema.json",
  "app_id": "miniapp-example",
  "name": "Example Multi-Chain App",
  "name_zh": "示例多链应用",
  "description": "A multi-chain miniapp example",
  "description_zh": "多链小程序示例",
  "category": "defi",
  "status": "active",
  "supported_chains": ["neo-n3-mainnet", "neo-n3-testnet", "neox-mainnet", "neox-testnet"],
  "contracts": {
    "neo-n3-mainnet": {
      "chain_id": "neo-n3-mainnet",
      "contract_address": "0x1234567890abcdef1234567890abcdef12345678",
