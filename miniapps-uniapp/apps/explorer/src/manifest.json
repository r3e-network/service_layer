{
  "name": "Neo Explorer",
  "appid": "miniapp-explorer",
  "description": "Explore Neo N3 blockchain - transactions, addresses, contracts",
  "versionName": "1.0.0",
  "versionCode": "100",
  "transformPx": false,
  "h5": {
    "title": "Neo Explorer",
    "router": {
      "mode": "hash"
    },
    "devServer": {
      "port": 5180
    }
  },
  "supported_chains": [
    "neo-n3-mainnet",
    "neo-n3-testnet"
  ],
  "name_zh": "Neo 浏览器",
  "description_zh": "探索 Neo N3 区块链 - 交易、地址、合约",
  "category": "tools",
  "tags": [
    "explorer",
    "blockchain",
    "transactions",
    "search"
  ],
  "permissions": [
    "datafeed"
  ],
  "assets_allowed": [],
  "limits": {},
  "contracts_needed": [],
  "network": {
    "contracts": {
      "testnet": null,
      "mainnet": null
    }
  },
  "card": {
    "display": {
      "type": "live_status",
      "banner": "/static/banner.svg",
      "live_status": {
        "enabled": true,
        "refresh_interval": 15,
        "fields": [
          "mainnet_height",
          "testnet_height",
          "mainnet_txs",
          "testnet_txs"
        ],
        "api_endpoint": "/api/explorer/stats"
      }
    },
    "info": {
      "logo": "/static/icon.svg",
      "owner": {
        "name": "R3E Network",
        "address": "NLtL2v28d7TyMEaXcPqtekunkFRksJ7wxu",
        "verified": true
      }
    }
  }
}