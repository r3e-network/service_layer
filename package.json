{
  "name": "neo-service-layer",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "dev": "turbo dev",
    "dev:host": "turbo dev --filter=meshminiapp-host",
    "dev:mobile": "turbo dev --filter=neo-miniapp-wallet",
    "build": "turbo build",
    "build:host": "turbo build --filter=meshminiapp-host",
    "build:mobile": "turbo build --filter=neo-miniapp-wallet",
    "build:miniapps": "turbo build:h5 --filter='./miniapps/*'",
    "postbuild:miniapps": "node scripts/copy-miniapps.mjs",
    "test": "turbo test",
    "test:run": "turbo test:run",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules/.cache"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "dotenv": "^17.2.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "sharp": "^0.34.5",
    "turbo": "^2.7.5",
    "typescript-eslint": "^8.53.1",
    "vite-plugin-node-polyfills": "^0.25.0"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.3.1",
      "react-dom": "^18.3.1",
      "react-is": "^18.3.1",
      "@types/react": "^18.3.1",
      "@types/react-dom": "^18.3.0",
      "jspdf": ">=4.0.0",
      "@intlify/core-base": ">=9.1.11",
      "@intlify/message-resolver": ">=9.1.11",
      "@intlify/vue-devtools>@intlify/message-resolver": ">=9.1.11",
      "esbuild": ">=0.25.0",
      "dompurify": ">=3.2.4",
      "lodash": ">=4.17.23",
      "ws": ">=8.17.1",
      "semver": ">=7.5.4",
      "postcss": ">=8.4.31",
      "jose": ">=5.2.0",
      "axios": ">=1.6.0",
      "cross-sos": ">=7.0.3",
      "cookie": ">=0.6.0",
      "tar": ">=7.5.7"
    }
  }
}
