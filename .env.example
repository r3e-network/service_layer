# Copy this file to `.env` and adjust values as needed.
# When DATABASE_URL is empty the runtime boots with in-memory storage.
DATABASE_URL=postgres://service_layer:service_layer@localhost:5432/service_layer?sslmode=disable
# Optional AES-GCM key for encrypting secrets (raw/base64/hex 16, 24, or 32 bytes).
SECRET_ENCRYPTION_KEY=
# Provide bearer tokens for HTTP authentication (comma separated for multiple tokens).
API_TOKENS=
# External integrations
PRICEFEED_FETCH_URL=
PRICEFEED_FETCH_KEY=
ORACLE_RESOLVER_URL=
ORACLE_RESOLVER_KEY=
GASBANK_RESOLVER_URL=
GASBANK_RESOLVER_KEY=
# Override the default log level (debug, info, warn, error).
LOG_LEVEL=info
