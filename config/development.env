# Development Environment Configuration
# MarbleRun runs in --insecure mode (no SGX required)

# Environment
MARBLE_ENV=development

# MarbleRun Coordinator
COORDINATOR_CLIENT_ADDR=localhost:4433
MARBLE_TYPE=gateway
MARBLE_DNS_NAMES=localhost

# MarbleRun Mode
MARBLERUN_INSECURE=true

# Neo N3 Network
NEO_RPC_URL=https://testnet1.neo.coz.io:443
NEO_NETWORK_MAGIC=894710606

# Supabase (Development Instance)
SUPABASE_URL=http://localhost:54321
# Service role key (keep secret). For local Supabase CLI you can get it via `supabase status`.
SUPABASE_SERVICE_KEY=your-local-supabase-service-role-key

# Service Ports
GATEWAY_PORT=8080
NEOFEEDS_PORT=8083
NEOFLOW_PORT=8084
NEOACCOUNTS_PORT=8085
NEOCOMPUTE_PORT=8086
ORACLE_PORT=8088
TXPROXY_PORT=8090
GLOBALSIGNER_PORT=8092

# Logging
LOG_LEVEL=debug
LOG_FORMAT=json

# Security (Development - Relaxed)
JWT_EXPIRY=24h
RATE_LIMIT_ENABLED=false
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Database
DB_MAX_CONNECTIONS=10
DB_IDLE_TIMEOUT=5m

# Development Features
ENABLE_PROFILING=true
ENABLE_DEBUG_ENDPOINTS=true
